includes:
    - ./vendor/larastan/larastan/extension.neon

parameters:
    paths:
        - app/

    # Level 0-9 (0が最も緩く、9が最も厳格)
    level: 5

    ignoreErrors:
        # 必要に応じて無視するエラーをここに追加

    excludePaths:
        - app/Console/Kernel.php
        - app/Exceptions/Handler.php

    checkMissingIterableValueType: false
